{
    "name": "xepozz/yii2-api-model-presenter",
    "type": "yii2-extension",
    "description": "The API model presenter.",
    "keywords": [
        "yii2",
        "api",
        "model",
        "response",
        "presenter",
        "serializer",
        "converter",
        "formatter"
    ],
    "authors": [
        {
            "name": "Dmitrii Derepko",
            "email": "xepozz@list.ru"
        }
    ],
    "require": {
        "php": ">=5.6",
        "yiisoft/yii2": "~2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4"
    },
    "autoload": {
        "psr-4": {
            "Xepozz\\Yii2ApiModelPresenter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xepozz\\Yii2ApiModelPresenter\\Tests\\": "tests"
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}